<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cs301.amazebyjosephpalazzo">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="cs301.amazebyjosephpalazzo.gui.AMazeActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="cs301.amazebyjosephpalazzo.gui.GeneratingActivity"></activity>
        <activity android:name="cs301.amazebyjosephpalazzo.gui.PlayManuallyActivity"></activity>
        <activity android:name="cs301.amazebyjosephpalazzo.gui.PlayAnimationActivity"></activity>
        <activity android:name="cs301.amazebyjosephpalazzo.gui.LosingActivity"></activity>
        <activity android:name="cs301.amazebyjosephpalazzo.gui.WinningActivity"></activity>

        <activity android:name="com.example.MarcAndChris.SkyrimMaze.FinishActivityLose"></activity>
    </application>

</manifest>